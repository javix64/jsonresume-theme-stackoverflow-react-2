{
  "name": "jsonresume-theme-stackoverflow-react",
  "version": "2.1.0",
  "description": "Stack Overflow theme for JSON Resume written with React",
  "author": "Levin Keller",
  "repository": {
    "type": "git",
    "url": "https://github.com/levino/jsonresume-theme-stackoverflow-react"
  },
  "license": "MIT",
  "main": "./dist/es/index.js",
  "exports": {
    ".": "./dist/en/index.js",
    "./spanish": "./dist/es/index.js",
    "./german": "./dist/de/index.js"
  },
  "dependencies": {
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "jsonresume-theme-stackoverflow": "^2.0.1",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@types/ramda": "^0.29.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prop-types": "^15.8.1",
    "resume-cli": "3.1.2",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "dev": "resume serve --theme .",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "README.md",
    "style.css"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
